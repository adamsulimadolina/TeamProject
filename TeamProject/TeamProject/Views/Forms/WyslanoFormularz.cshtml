@model List<FieldWithValue>
@{
    ViewData["Title"] = "WyslanoFormularz";
}
<div><br /></div>
<span>
    <h2>Wysłano Formularz</h2>
</span>

<style>

    .cities {
        color: white;
        margin: 20px;
        padding: 20px;
    }
</style>
<div class="cities">
    @foreach (var key in Model)
    {
        <label asp-for="@key.Field.Name" style="font-weight:bold; font-size:large">@key.Field.Name</label>
        @if (key.Field.Type == "checkbox")
        {
            <p>@key.BoolValue.ToString()</p>
        }
        else
        {
            <p>@key.TextValue</p>
        }
        <br />
    }
</div>



