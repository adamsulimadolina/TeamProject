@model IEnumerable<FormGenerator.Models.Patient>
@{
    ViewData["Title"] = "View";
}

<h1>View</h1>






<form id="formA" action="Patient/Index" method="post">
    <input name="Id" id="searchPatientv2" />
    <input type="submit" id="buttonSearch" name="Szukaj" />
</form>

@TempData["Message"]



<div class="alert alert-warning alert-dismissible fade show" role="alert">
    <strong>UWAGA!</strong> @TempData["Messagenie"]
    <button type="button" class="close" data-dismiss="alert" aria-label="Close">
        <span aria-hidden="true">&times;</span>
    </button>
</div>

<script src="~/lib/jquery/dist/jquery.min.js"></script>
<script>
    $('#formA').submit(function () {
        sessionStorage.setItem('key', document.getElementById("searchPatientv2").value);


    });
</script>


