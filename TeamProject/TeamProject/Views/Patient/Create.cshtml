@model FormGenerator.Models.Patient
    <script>
        sessionStorage.removeItem('key');
    </script>
<div><a asp-action="Index" asp-controller="Patient" class="btn btn-danger">Powrót do menu głównego</a></div>
<h3>Dodaj nowego pacjenta</h3>

<div class="row">
    <div class="col-md-4 offset-4">
        <form asp-action="Create">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group">
                <label asp-for="IdPatient" class="control-label">Podaj Id pacienta</label>
                <input asp-for="IdPatient" class="form-control" value=0 />
                <span asp-validation-for="IdPatient" class="text-danger"></span>
            </div>


            <div class="form-group">
                <input type="submit" value="Stwórz" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>



@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
